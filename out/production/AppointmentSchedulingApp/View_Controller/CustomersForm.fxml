<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="467.0" prefWidth="832.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.CustomersFormController">
   <children>
      <TableView fx:id="customersTbl" editable="true" prefHeight="362.0" prefWidth="851.0">
        <columns>
          <TableColumn fx:id="customerIDCol" prefWidth="88.0" text="Customer ID" />
          <TableColumn fx:id="customerNameCol" prefWidth="170.0" text="Name" />
            <TableColumn fx:id="addressCol" prefWidth="170.0" text="Address" />
            <TableColumn fx:id="postalCodeCol" minWidth="0.0" prefWidth="95.0" text="Postal" />
            <TableColumn fx:id="divisionCol" prefWidth="135.0" text="Division" />
            <TableColumn fx:id="countryCol" prefWidth="92.0" text="Country" />
            <TableColumn fx:id="phoneCol" prefWidth="100.0" text="Phone" />
        </columns>
      </TableView>
      <Button fx:id="cancelBtn" layoutX="706.0" layoutY="394.0" mnemonicParsing="false" onAction="#returnOptionsForm" prefHeight="46.0" prefWidth="96.0" text="Cancel" />
      <Button fx:id="addCustomerBtn" layoutX="52.0" layoutY="394.0" mnemonicParsing="false" onAction="#displayAddCustomerForm" prefHeight="46.0" prefWidth="96.0" text="Add" />
      <Button fx:id="modifyCustomerBtn" layoutX="202.0" layoutY="394.0" mnemonicParsing="false" onAction="#displayModifyCustomerForm" prefHeight="46.0" prefWidth="96.0" text="Modify" />
      <Button fx:id="deleteCustomerBtn" layoutX="352.0" layoutY="394.0" mnemonicParsing="false" onAction="#deleteCustomer" prefHeight="46.0" prefWidth="96.0" text="Delete" />
   </children>
</AnchorPane>
